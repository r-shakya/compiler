-------if----------
t1=var1<var2
if not t1 goto l0
var3=var1+var2;
l0:








----------if else----------
t1=var1<var2
if not t1 goto l0
var3=var1+var2;
goto l1
l0:
var3=0;
l1:



----------while------------
l0:
t1=var1<var2
if not t1 goto l1
var3=var1+var2;
goto l0
l1:





----------for------------
t0 = 0
i = t0
l0: 
t1 = i
t2 = 5
t3 = t1 < t2
if not t3 goto l3
goto l2

l1:
t4 = i
t5 = 1
t6 = t4 + t5
i = t6 
goto l0

l2:
t7 = var1
t8 = i
t9 = t7 + t8
var3 = t9 
goto l1

l3:

----






----------access  array ------

ar[2] =  4

t-1 = 2
t0 = &ar
t1 = 4 * t-1
t2 = t1 + t0
t4 = 4
*t2 = t4

 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 









